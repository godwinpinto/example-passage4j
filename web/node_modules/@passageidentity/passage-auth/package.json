{
  "name": "@passageidentity/passage-auth",
  "version": "0.1.0-beta.28",
  "private": false,
  "main": "dist/package/index.cjs.js",
  "module": "dist/package/index.es.js",
  "types": "dist/package/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "node": "./dist/package/index.es.js",
        "default": "./dist/package/index.es.js"
      },
      "require": "./dist/package/index.cjs.js",
      "types": "./dist/package/index.d.js"
    },
    "./passage-user": {
      "import": "./dist/package/PassageUser/index.mjs",
      "require": "./dist/package/PassageUser/index.js",
      "types": "./dist/package/PassageUser/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "browser": {
    "./passage-user": "./dist/package/PassageUser/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/package/index.d.ts"
      ],
      "passage-user": [
        "./dist/package/PassageUser/index.d.ts"
      ]
    }
  },
  "scripts": {
    "check": "vue-tsc --noEmit",
    "build": "npm run build:package && npm run build:api && npm run build:cdn ",
    "build:dev": "APP_ENV=dev npm run build",
    "build:uat": "APP_ENV=uat npm run build",
    "build:package": "npm run check && vite build && npm run build:types",
    "build:types": "tsup ./src/index.ts --dts-only --outDir='dist/package'",
    "build:api": "tsup ./src/api/PassageUser/index.ts --format esm,cjs --dts-resolve --minify --outDir='dist/package/PassageUser'",
    "build:cdn": " vite build --config vite.cdn.config.js",
    "build:watch": "vite build --config vite.cdn.config.js --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "preview": "vite preview --config vite.cdn.config.js --host",
    "serve": "npm run build:watch & npm run preview",
    "lint": "eslint . --ext .ts,.js,.vue src",
    "lint:fix": "eslint . --ext .ts,.js,.vue src --fix",
    "prepare": "husky install && chmod +x .husky/pre-commit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "change": "beachball change",
    "publish": "beachball publish -y -n",
    "beachball-check": "beachball check"
  },
  "dependencies": {
    "@passageidentity/passage-js": "^2.0.2",
    "bulma": "^0.9.3",
    "intl-tel-input": "^17.0.13",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@types/intl-tel-input": "^17.0.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/cli-plugin-unit-jest": "^4.5.14",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "beachball": "^2.16.0",
    "dotenv": "^10.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.14.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.0",
    "sass": "^1.43.2",
    "tsup": "^5.11.11",
    "typescript": "^4.3.5",
    "vite": "^2.6.14",
    "vue-jest": "^5.0.0-0",
    "vue-tsc": "^0.28.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
